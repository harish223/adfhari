{
	"name": "FIXED LENGTH",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "FIXEDLENGTH_1",
						"type": "DatasetReference"
					},
					"name": "FIXEDLENGTH"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {001John     Doe       30MNew York   USA     } as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> FIXEDLENGTH",
				"FIXEDLENGTH derive(EMP_ID = substring(Column1,1,3),",
				"          FIRST_NAME = substring(Column1, 4,10),",
				"          LAST_NAME = substring(Column1,15,10),",
				"          column1 = ERROR_FUNCTION('')) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          {001John     Doe       30MNew York   USA     },",
				"          EMP_ID,",
				"          FIRST_NAME,",
				"          LAST_NAME,",
				"          column1",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}